function [result1, result2] = make_dominant(n, eqs)
    [A, B] = extract_matrix(n, eqs);
    result1 = is_dominant(A);
    if(result1)
        return;
    end
    
end

function result = is_dominant(A)
    n = size(A, 1);
    result = true;
    for i = 1 : n
        sumrem = 0;
        sumdiag = 0;
        for j = 1 : n
            if(i == j)
                sumdiag = sumdiag + abs(A(i,j));
            else
                sumrem = sumrem + abs(A(i, j));
            end
        end
        if(sumrem >= sumdiag)
            result = false;
        end
    end
end

